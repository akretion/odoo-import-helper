<odoo>
    <record id="import_show_logs_form" model="ir.ui.view">
        <field name="model">import.show.logs</field>
        <field name="arch" type="xml">
            <form>
                <group name="main">
                    <field name="logs" nolabel="1" colspan="2" />
                </group>
                <footer>
                    <button string="Close" class="btn-primary" special="cancel" />
                </footer>
            </form>
        </field>
    </record>
</odoo>
